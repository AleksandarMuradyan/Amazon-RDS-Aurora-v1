--
-- Name: customer_list; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW pagilasakila.customer_list AS
 SELECT cu.customer_id AS id,
    ((cu.first_name || ' '::text) || cu.last_name) AS name,
    a.address,
    a.postal_code AS "zip code",
    a.phone,
    city.city,
    country.country,
        CASE
            WHEN cu.activebool THEN 'active'::text
            ELSE ''::text
        END AS notes,
    cu.store_id AS sid
   FROM (((pagilasakila.customer cu
     JOIN pagilasakila.address a ON ((cu.address_id = a.address_id)))
     JOIN pagilasakila.city ON ((a.city_id = city.city_id)))
     JOIN pagilasakila.country ON ((city.country_id = country.country_id)));


ALTER TABLE pagilasakila.customer_list OWNER TO masteruser;

--
-- Name: film_list; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW pagilasakila.film_list AS
 SELECT film.film_id AS fid,
    film.title,
    film.description,
    category.name AS category,
    film.rental_rate AS price,
    film.length,
    film.rating,
    pagilasakila.group_concat(((actor.first_name || ' '::text) || actor.last_name)) AS actors
   FROM ((((pagilasakila.category
     LEFT JOIN pagilasakila.film_category ON ((category.category_id = film_category.category_id)))
     LEFT JOIN pagilasakila.film ON ((film_category.film_id = film.film_id)))
     JOIN pagilasakila.film_actor ON ((film.film_id = film_actor.film_id)))
     JOIN pagilasakila.actor ON ((film_actor.actor_id = actor.actor_id)))
  GROUP BY film.film_id, film.title, film.description, category.name, film.rental_rate, film.length, film.rating;


ALTER TABLE pagilasakila.film_list OWNER TO masteruser;

