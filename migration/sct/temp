--

CREATE TABLE pagilasakila.store (
    store_id integer DEFAULT nextval('pagilasakila.store_store_id_seq'::regclass) NOT NULL,
    manager_staff_id integer NOT NULL,
    address_id integer NOT NULL,
    last_update timestamp with time zone DEFAULT now() NOT NULL
);


ALTER TABLE pagilasakila.store OWNER TO masteruser;

--
-- Name: sales_by_store; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW pagilasakila.sales_by_store AS
 SELECT ((c.city || ','::text) || cy.country) AS store,
    ((m.first_name || ' '::text) || m.last_name) AS manager,
    sum(p.amount) AS total_sales
   FROM (((((((pagilasakila.payment p
     JOIN pagilasakila.rental r ON ((p.rental_id = r.rental_id)))
     JOIN pagilasakila.inventory i ON ((r.inventory_id = i.inventory_id)))
     JOIN pagilasakila.store s ON ((i.store_id = s.store_id)))
     JOIN pagilasakila.address a ON ((s.address_id = a.address_id)))
     JOIN pagilasakila.city c ON ((a.city_id = c.city_id)))
     JOIN pagilasakila.country cy ON ((c.country_id = cy.country_id)))
     JOIN pagilasakila.staff m ON ((s.manager_staff_id = m.staff_id)))
  GROUP BY cy.country, c.city, s.store_id, m.first_name, m.last_name
  ORDER BY cy.country, c.city;


ALTER TABLE pagilasakila.sales_by_store OWNER TO masteruser;

--
-- Name: staff_list; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW pagilasakila.staff_list AS
 SELECT s.staff_id AS id,
    ((s.first_name || ' '::text) || s.last_name) AS name,
    a.address,
    a.postal_code AS "zip code",
    a.phone,
    city.city,
    country.country,
    s.store_id AS sid
   FROM (((pagilasakila.staff s
     JOIN pagilasakila.address a ON ((s.address_id = a.address_id)))
     JOIN pagilasakila.city ON ((a.city_id = city.city_id)))
     JOIN pagilasakila.country ON ((city.country_id = country.country_id)));


ALTER TABLE pagilasakila.staff_list OWNER TO masteruser;

