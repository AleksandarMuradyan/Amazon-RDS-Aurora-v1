-- Name: payment_p2020_04; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE pagilasakila.payment_p2020_04 (
    payment_id integer DEFAULT nextval('pagilasakila.payment_payment_id_seq'::regclass) NOT NULL,
    customer_id integer NOT NULL,
    staff_id integer NOT NULL,
    rental_id integer NOT NULL,
    amount numeric(5,2) NOT NULL,
    payment_date timestamp with time zone NOT NULL
);
ALTER TABLE ONLY pagilasakila.payment ATTACH PARTITION pagilasakila.payment_p2020_04 FOR VALUES FROM ('2020-04-01 01:00:00+01') TO ('2020-05-01 01:00:00+01');


ALTER TABLE pagilasakila.payment_p2020_04 OWNER TO masteruser;

