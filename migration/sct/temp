--
-- Name: nicer_but_slower_film_list; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW pagilasakila.nicer_but_slower_film_list AS
 SELECT film.film_id AS fid,
    film.title,
    film.description,
    category.name AS category,
    film.rental_rate AS price,
    film.length,
    film.rating,
    pagilasakila.group_concat((((upper("substring"(actor.first_name, 1, 1)) || lower("substring"(actor.first_name, 2))) || upper("substring"(actor.last_name, 1, 1))) || lower("substring"(actor.last_name, 2)))) AS actors
   FROM ((((pagilasakila.category
     LEFT JOIN pagilasakila.film_category ON ((category.category_id = film_category.category_id)))
     LEFT JOIN pagilasakila.film ON ((film_category.film_id = film.film_id)))
     JOIN pagilasakila.film_actor ON ((film.film_id = film_actor.film_id)))
     JOIN pagilasakila.actor ON ((film_actor.actor_id = actor.actor_id)))
  GROUP BY film.film_id, film.title, film.description, category.name, film.rental_rate, film.length, film.rating;


ALTER TABLE pagilasakila.nicer_but_slower_film_list OWNER TO masteruser;

