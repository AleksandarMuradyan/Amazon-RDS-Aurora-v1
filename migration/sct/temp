--
-- Name: sales_by_film_category; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW pagilasakila.sales_by_film_category AS
 SELECT c.name AS category,
    sum(p.amount) AS total_sales
   FROM (((((pagilasakila.payment p
     JOIN pagilasakila.rental r ON ((p.rental_id = r.rental_id)))
     JOIN pagilasakila.inventory i ON ((r.inventory_id = i.inventory_id)))
     JOIN pagilasakila.film f ON ((i.film_id = f.film_id)))
     JOIN pagilasakila.film_category fc ON ((f.film_id = fc.film_id)))
     JOIN pagilasakila.category c ON ((fc.category_id = c.category_id)))
  GROUP BY c.name
  ORDER BY (sum(p.amount)) DESC;


ALTER TABLE pagilasakila.sales_by_film_category OWNER TO masteruser;

