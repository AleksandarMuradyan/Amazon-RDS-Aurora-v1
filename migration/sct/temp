--
-- Name: actor_info; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW pagilasakila.actor_info AS
 SELECT a.actor_id,
    a.first_name,
    a.last_name,
    pagilasakila.group_concat(DISTINCT ((c.name || ': '::text) || ( SELECT pagilasakila.group_concat(f.title) AS group_concat
           FROM ((pagilasakila.film f
             JOIN pagilasakila.film_category fc_1 ON ((f.film_id = fc_1.film_id)))
             JOIN pagilasakila.film_actor fa_1 ON ((f.film_id = fa_1.film_id)))
          WHERE ((fc_1.category_id = c.category_id) AND (fa_1.actor_id = a.actor_id))
          GROUP BY fa_1.actor_id))) AS film_info
   FROM (((pagilasakila.actor a
     LEFT JOIN pagilasakila.film_actor fa ON ((a.actor_id = fa.actor_id)))
     LEFT JOIN pagilasakila.film_category fc ON ((fa.film_id = fc.film_id)))
     LEFT JOIN pagilasakila.category c ON ((fc.category_id = c.category_id)))
  GROUP BY a.actor_id, a.first_name, a.last_name;


ALTER TABLE pagilasakila.actor_info OWNER TO masteruser;

